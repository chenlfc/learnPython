	// }
	"My title": {
		"prefix": "_for_ex",
		"body": [
			"# -*- coding: utf-8 -*-",
			"# cretae by o.c. $1/$2/$3",
			"# from _for_ex import pt_title",
			"",
			"# pt_title('$4')"
		],
		"description": "插入自定义的标题打印函数"
	},
	"My title-2": {
		"prefix": "__for_other",
		"body": [
			"# -*- coding: utf-8 -*-",
			"# create by o.c. $1/$2/$3",
			"# file name : $4",
			"# description : $5",
			"",
			""
		],
		"description": "插入自定义标题注释"
	}
}